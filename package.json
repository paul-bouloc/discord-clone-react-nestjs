{
  "name": "discord-clone-react-nestjs",
  "private": true,
  "scripts": {
    "dev": "pnpm -r --parallel dev",
    "build": "pnpm -r build",
    "lint": "pnpm -r lint",
    "test": "pnpm -r test",
    "typecheck": "pnpm -r typecheck",
    "db:up": "docker compose up -d postgres",
    "db:down": "docker compose down postgres",
    "db:logs": "docker compose logs -f postgres",
    "db:reset": "docker compose down -v postgres && docker compose up -d postgres",
    "pgadmin:up": "docker compose up -d pgadmin",
    "pgadmin:down": "docker compose down pgadmin",
    "docker:up": "docker compose up -d",
    "docker:down": "docker compose down",
    "docker:logs": "docker compose logs -f"
  }
}
